class Movable {
    void move() {
        System.out.println("Moving...");
    }
}
interface Drivable {
    void drive();
}
interface Flyable {
    void fly();
}
class Vehicle extends Movable implements Drivable, Flyable {
    void drive() {
        System.out.println("Driving...");
    }
    void fly() {
        System.out.println("Flying...");
    }
}
public class Main {
    public static void main(String[] args) {
        Vehicle vehicle = new Vehicle();
        vehicle.move();
        vehicle.drive();
        vehicle.fly();
    }
}
